! Scrollbar
URxvt.scrollBar:		false
! Cursor
URxvt.cursorBlink:		true
! Font Einstellungen
URxvt.font:				xft:monospace:size=14

URxvt.intensityStyles:	false
URxvt.background:		#000000
URxvt.cursorColor:		white

! Fake Transparency
!URxvt.transparent:		true
!URxvt.shading:			30
! Real Transparency
urxvt*depth:			32
urxvt*background:		rgba:0000/0000/0200/c800

URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:			#4682b4
! URL-Select
URxvt.keysym.M-u:		perl:url-select:select_next
URxvt.url-select.laucher:   chromium
URxvt.url.select.underline: true

! Fontgroesse aendern
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt.url-launcher:		chromium
URxvt.matcher.button:	1

! rofi Einstellungen
rofi.color-enabled:		true
rofi.transparency:		true
rofi.lines:				5
rofi.bw:				0
rofi.hide-scrollbar:	true
rofi.width:				40
rofi.font:				System San Francisco Display Regular 18
rofi.padding:			50

rofi.color-window:		argb:DD000000, argb:DD000000, argb:DD000000
rofi.color-normal:		argb:00000000, #819396, argb:00000000, #008ed4
rofi.color-active:		#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:		#002b37, #da4281, #003643, #008ed4, #890661
